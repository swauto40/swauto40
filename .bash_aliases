
# Custom aliases
alias c="clear"
alias h="history 20"
alias q="exit"
alias ai="sudo apt install"
alias ap="sudo apt purge"
alias up="sudo apt update && sudo apt upgrade -y && sudo apt autoremove --purge -y"
alias png="ping bing.com"
alias wt="curl wttr.in/Cebu_City"
alias myip="curl https://wtfismyip.com/text"
alias sp="speedtest-cli --single --secure"
alias vcm="veracrypt --mount /dev/sdc1"
alias vcd="veracrypt -d"
alias a2c="aria2c -x16"
alias gldl="gallery-dl --retries 50 --filesize-max 1M"
alias ytdl="youtube-dl --retries 50 --fragment-retries 50 --no-playlist --ignore-errors --socket-timeout 20 --format "best[height<=720]" --external-downloader aria2c --external-downloader-args "-x16" "
alias ext="engrampa -h"






